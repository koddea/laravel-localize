{
    "name": "koddea/laravel-localize",
    "description": "A Laravel package for localization purposes",
    "keywords": [
        "laravel-localize",
        "laravel",
        "translation",
        "route",
        "localization",
        "lang",
        "php"
    ],
    "homepage": "https://github.com/koddea/laravel-localize",
    "license": "MIT",
    "authors": [{
        "name": "Emrah Ar"
    }],
    "require": {
        "php": "^7.1 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Koddea\\Localize\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Koddea\\Localize\\LocalizeServiceProvider"
            ],
            "aliases": {
                "Loc": "Koddea\\Localize\\Facades\\Loc"
            }
        }
    }
}
